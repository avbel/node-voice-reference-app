{
  "name": "Voice reference app",
  "description": "It demonstrates how to implement voice calling for mobile devices, browsers (WebRTC), and any SIP client using Catapult API.",
  "repository": "https://github.com/BandwidthExamples/node-voice-reference-app.git",
  "logo": "https://avatars2.githubusercontent.com/u/12057884?v=3&s=200",
  "keywords": ["node", "bandwidth", "catapult"],
  "env": {
    "CATAPULT_USER_ID": {
      "description": "Your Catapult User Id"
    },
    "CATAPULT_API_TOKEN": {
      "description": "Your Catapult Api Token"
    },
    "CATAPULT_API_SECRET": {
      "description": "Your Catapult Api Secret"
    },
    "CATAPULT_DOMAIN_NAME": {
      "description": "Your Catapult Domain Name"
    }
  },
  "addons": ["mongolab"]
}
